  "private": true,
  "author": {
    "name": "Jonas Maccyber Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "files": [
    "src/*"
  ],
  "main": "src/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "micro-dev -H localhost",
    "start": "micro",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/tfk-elevinfo-api.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tfk-elevinfo-api/issues"
  },
  "homepage": "https://github.com/telemark/tfk-elevinfo-api#readme",
  "devDependencies": {
    "ava": "1.4.1",
    "coveralls": "3.0.3",
    "husky": "2.2.0",
    "micro-dev": "3.0.0",
    "nyc": "14.1.0",
    "standard": "12.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "cors": "2.8.5",
    "finalhandler": "1.1.2",
    "jsonwebtoken": "8.5.1",
    "markdown-it": "8.4.2",
